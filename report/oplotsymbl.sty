%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Definition of half circle "halfcircletlet" (filled)
\tikzset{
  halfcirclet/.pic={
    \clip [preaction={draw,line width=0.05em}] (0,0) circle [radius=.34em];
    \fill (0,0)--+(#1:.34em)arc[start angle=#1,delta angle=180,radius=.34em];
  }
}
